--TEST--
igbinary and large arrays
--FILE--
<?php
$a = range(0,255);
var_dump(bin2hex($s = igbinary_serialize($a)));
var_dump(igbinary_unserialize($s) === $a);
$largeArray = range(0, 1 << 16);
var_dump(igbinary_unserialize(igbinary_serialize($largeArray)) === $largeArray);
?>
--EXPECTF--
string(2062) ""
bool(true)
bool(true)
